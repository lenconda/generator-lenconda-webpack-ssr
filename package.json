{
  "name": "@lenconda/generator-react-ssr",
  "version": "0.2.0",
  "description": "A generator for server-side rendering with React and Webpack",
  "homepage": "https://github.com/lenconda/generator-react-ssr",
  "author": {
    "name": "lenconda",
    "email": "i@lenconda.top",
    "url": "https://blog.lenconda.top"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "Webpack",
    "SSR",
    "yeoman-generator"
  ],
  "devDependencies": {
    "coveralls": "^3.0.5",
    "eslint": "^6.0.1",
    "eslint-config-alloy": "^3.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "babel-eslint": "^10.0.3",
    "chalk": "^2.1.0",
    "mkdirp": "^0.5.1",
    "yeoman-generator": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "exit 0"
  },
  "repository": "lenconda/generator-lenconda-webpack-ssr",
  "license": "MIT"
}
